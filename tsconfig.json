{
    "compilerOptions": {
        "target": "ESNext",
        "module": "ESNext",
        "lib": [
            "DOM",
            "ESNext"
        ],
        "moduleResolution": "Node",
        "strict": true,
        "jsx": "react",
        "skipLibCheck": true,
        "esModuleInterop": true,
        "resolveJsonModule": true
    },
    "include": [
        "**/*.ts",
        "**/*.tsx",
        "**/*.vue",
    ],
    "exclude": [
        "node_modules"
    ]
}